{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Claude Code Router",
  "version": "1.0.0",
  "identifier": "com.claude-code-router.app",
  "build": {
    "beforeDevCommand": "cd /Users/sunjackson/Project/claude-code-router/src-ui && npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "cd /Users/sunjackson/Project/claude-code-router/src-ui && npm run build",
    "frontendDist": "../src-ui/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "windows": [
      {
        "title": "Claude Code Router",
        "width": 1280,
        "height": 800,
        "minWidth": 1024,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
